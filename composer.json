{
  "name": "laravel-mq/rabbit",
  "description": "Reusable RabbitMQ integration using php-amqplib for Laravel apps.",
  "type": "library",
  "license": "MIT",
  "require": {
    "php": "^8.4",
    "illuminate/support": "^12.0",
    "illuminate/console": "^12.0",
    "illuminate/log": "^12.0",
    "php-amqplib/php-amqplib": "^3.7"
  }
  ,
  "require-dev": {
    "roave/security-advisories": "dev-latest",
    "laravel/pint": "^1.22"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Starematic\\RabbitMQ\\RabbitMQServiceProvider"
      ]
    }
  },
  "autoload": {
    "psr-4": {
      "Starematic\\RabbitMQ\\": "src/"
    }
  },
  "scripts": {
    "format": "pint"
  }
}
